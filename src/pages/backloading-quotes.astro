---
import Layout from '../layouts/Layout.astro';

// Redirect to external quote system
return Astro.redirect('https://removalistquotes.movingagain.com.au/quote/household', 302);
---

<Layout title="Redirecting...">
  <div class="container mx-auto px-4 py-20 text-center">
    <p>Redirecting to quote form...</p>
    <a
      href="https://removalistquotes.movingagain.com.au/quote/household"
      class="text-brand-red hover:underline"
    >
      Click here if not redirected
    </a>
  </div>
</Layout>
